<?xml version="1.0" encoding="UTF-8"?>
<proxy name="UTEncryptedProxy" startOnLoad="true" transports="http" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <header action="remove" name="wsse:Security" scope="default" xmlns:wsse="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd"/>
            <payloadFactory description="response" media-type="xml">
                <format>
                    <message xmlns="">Response from UT Encrypted Secured Service</message>
                </format>
                <args/>
            </payloadFactory>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
    <policy key="gov:security/UTEncrypted.xml"/>
    <enableSec/>
</proxy>
